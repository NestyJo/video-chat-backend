{
  "name": "agora-backend",
  "version": "1.0.0",
  "description": "Node.js backend application with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "check-db": "node scripts/check-mongodb.js",
    "setup-db": "echo 'Please see scripts/setup-mongodb.md for MongoDB setup instructions'",
    "seed": "node scripts/seed.js",
    "seed:admin": "node scripts/seed.js --admin",
    "seed:samples": "node scripts/seed.js --samples",
    "seed:clear": "node scripts/seed.js --clear --samples",
    "seed:help": "node scripts/seed.js --help",
    "test:api": "node scripts/test-api.js",
    "test:api:bash": "bash scripts/test-api.sh",
    "fix-admin": "node scripts/quick-admin-fix.js",
    "test-login": "node scripts/test-login.js",
    "verify-setup": "node scripts/verify-setup.js"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "backend",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.18.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "mongodb-memory-server": "^9.1.3",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  }
}
